{
  "chains": {
    "mainnet": {
      "chain": {
        "class": "populus.chain.external.ExternalChain"
      },
      "contracts": {
        "backends": {
          "JSONFile": {
            "class": "populus.contracts.backends.filesystem.JSONFileBackend",
            "priority": 10,
            "settings": {
              "file_path": "./registrar.json"
            }
          },
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.rpc.HTTPProvider",
          "settings": {
            "endpoint_uri": "http://127.0.0.1:8545",
            "request_kwargs": {
                "timeout": 180
            }
          }
        }
      }
    },
    "kovan": {
      "chain": {
        "class": "populus.chain.external.ExternalChain"
      },
      "contracts": {
        "backends": {
          "JSONFile": {
            "class": "populus.contracts.backends.filesystem.JSONFileBackend",
            "priority": 10,
            "settings": {
              "file_path": "./registrar.json"
            }
          },
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.rpc.HTTPProvider",
          "settings": {
            "endpoint_uri": "http://localhost:8547",
            "request_kwargs": {
                "timeout": 180
            }
          }
        }
      }
    },
    "local": {
      "chain": {
        "class": "populus.chain.geth.LocalGethChain",
        "settings": {
          "rpc_port": "8548"
        }
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.rpc.HTTPProvider",
          "settings": {
            "endpoint_uri": "http://127.0.0.1:8548",
            "request_kwargs": {
                "timeout": 180
            }
          }
        }
      }
    },
    "ganache": {
      "chain": {
        "class": "populus.chain.external.ExternalChain"
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.rpc.HTTPProvider",
          "settings": {
            "endpoint_uri": "http://172.21.0.2:8545",
            "request_kwargs": {
              "timeout": 180
            }
          }
        }
      }
    },
    "ropsten": {
      "chain": {
        "class": "populus.chain.external.ExternalChain"
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.rpc.HTTPProvider",
          "settings": {
            "endpoint_uri": "http://127.0.0.1:8546",
            "request_kwargs": {
                "timeout": 180
            }
          }
        }
      }
    },
    "temp": {
      "chain": {
        "class": "populus.chain.geth.TemporaryGethChain"
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.rpc.HTTPProvider",
          "settings": {
            "endpoint_uri": "http://127.0.0.1:8547"
          }
        }
      }
    },
    "tester": {
      "chain": {
        "class": "populus.chain.tester.TesterChain",
         "wait": {
           "settings": {
              "timeout": 10000
           }
         }
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.tester.EthereumTesterProvider"
        }
      }
    }
  },
  "compilation": {
    "backend": {
      "class": "populus.compilation.backends.SolcStandardJSONBackend",
      "settings": {
         "command_line_options": {
            "allow_paths": "/"
         },
        "optimize": true,
        "output_values": [
          "abi",
          "bin",
          "bin-runtime",
          "metadata"
        ]
      }
    },
    "backends": {
      "SolcAutoBackend": {
        "class": "populus.compilation.backends.SolcAutoBackend",
        "settings": {
          "command_line_options": {
            "allow_paths": "/"
          },
          "stdin": {
            "output_values": [
              "abi",
              "bin",
              "bin-runtime",
              "metadata"
            ],

            "optimizer": {
              "enabled": true,
              "runs": 500
            },
            "outputSelection": {
              "*": {
                "*": [
                  "abi",
                  "metadata",
                  "evm"
                 ]
              }
            }
          }
        }
      },
      "SolcCombinedJSON": {
        "class": "populus.compilation.backends.SolcCombinedJSONBackend",
        "settings": {
          "optimize": true,
          "output_values": [
            "abi",
            "bin",
            "bin-runtime",
            "metadata"
          ]
        }
      },
      "SolcStandardJSON": {
        "class": "populus.compilation.backends.SolcStandardJSONBackend",
        "settings": {
          "optimize": true,
          "output_values": [
            "abi",
            "bin",
            "bin-runtime",
            "metadata"
          ],
          "stdin": {
            "optimizer": {
              "enabled": true,
              "runs": 500
            },
            "outputSelection": {
              "*": {
                "*": [
                  "abi",
                  "metadata",
                  "evm"
                ]
              }
            }
          },
          "command_line_options": {
            "allow_paths": "/"
          }
        }
      }
    },
    "contract_source_dirs": [
      "./contracts"
    ],
    "import_remappings": ["zeppelin=zeppelin"]
  },
  "contracts": {
    "backends": {
      "JSONFile": {
        "class": "populus.contracts.backends.filesystem.JSONFileBackend",
        "priority": 10,
        "settings": {
          "file_path": "./registrar.json"
        }
      },
      "Memory": {
        "class": "populus.contracts.backends.memory.MemoryBackend",
        "priority": 50
      },
      "ProjectContracts": {
        "class": "populus.contracts.backends.project.ProjectContractsBackend",
        "priority": 20
      },
      "TestContracts": {
        "class": "populus.contracts.backends.testing.TestContractsBackend",
        "priority": 40
      }
    }
  },
  "version": "9",
  "web3": {
    "GethIPC": {
      "provider": {
        "class": "web3.providers.ipc.IPCProvider"
      }
    },
    "InfuraMainnet": {
      "eth": {
        "default_account": "0x0000000000000000000000000000000000000001"
      },
      "provider": {
        "class": "web3.providers.rpc.HTTPProvider",
        "settings": {
          "endpoint_uri": "https://mainnet.infura.io"
        }
      }
    },
    "InfuraRopsten": {
      "eth": {
        "default_account": "0x0000000000000000000000000000000000000001"
      },
      "provider": {
        "class": "web3.providers.rpc.HTTPProvider",
        "settings": {
          "endpoint_uri": "https://ropsten.infura.io"
        }
      }
    },
    "Ganache": {
      "provider": {
        "class": "web3.providers.tester.TesterChain",
        "settings": {
          "endpoint_uri": "http://172.21.0.2:8545"
        }
      }
    },
    "Tester": {
      "provider": {
        "class": "web3.providers.eth_tester.EthereumTesterProvider"
      }
    }
  }
}
